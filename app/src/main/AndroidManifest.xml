<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="id.fauzanag.ppdbsekolah">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".ui.student.RegisterStudentFourActivity"></activity>
        <activity
            android:name=".ui.splahscreen.SplashScreenActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.account.login.LoginActivity"
            android:theme="@style/AppTheme.AllUI" />
        <activity
            android:name=".ui.account.register.RegisterActivity"
            android:theme="@style/AppTheme.AllUI" />
        <activity
            android:name=".ui.onboarding.one.OneBoardingActivity"
            android:theme="@style/AppTheme.AllUI" />
        <activity
            android:name=".ui.onboarding.two.TwoBoardingActivity"
            android:theme="@style/AppTheme.AllUI" />
        <activity
            android:name=".ui.student.RegisterStudentOneActivity"
            android:theme="@style/AppTheme.AllUI" />
        <activity
            android:name=".ui.student.RegisterStudentTwoActivity"
            android:theme="@style/AppTheme.AllUI" />
        <activity
            android:name=".ui.student.RegisterStudentThreeActivity"
            android:theme="@style/AppTheme.AllUI" />
    </application>

</manifest>